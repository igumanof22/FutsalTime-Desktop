	<queryString>
		<![CDATA[SELECT
	date_format(penyewaan.`tglmain`,'%M')AS bulan,
     pelanggan.`namaplg` AS pelanggan_namaplg,
     penyewaan.`kdpemakaian` AS penyewaan_kdpemakaian,
     penyewaan.`tglmain` AS penyewaan_tglmain,
     penyewaan.`jamawal` AS penyewaan_jamawal,
     penyewaan.`jamakhir` AS penyewaan_jamakhir,
     penyewaan.`totalsewa` AS penyewaan_totalsewa,
     penyewaan.`pemasukkan` AS penyewaan_pemasukkan,
     penyewaan.`kdlapangan` AS penyewaan_kdlapangan,
     penyewaan.`kdpelanggan` AS penyewaan_kdpelanggan
FROM
     `pelanggan` pelanggan INNER JOIN `penyewaan` penyewaan ON pelanggan.`kdpelanggan` = penyewaan.`kdpelanggan`
WHERE
	month(tglmain) = $P{pbulan}
	and year(tglmain) = $P{ptahun}]]>
	</queryString>
